#!/usr/bin/env python

import sys,re

from Bio import SeqIO

output = open("Filtered_Seq2.fasta", "w")

wanted = set(line.rstrip("\n").split(None,1)[0] for line in open("UniqueGeneNames.txt","r"))

records = (r for r in SeqIO.parse(open("tapeworm_proteins.fa", "r"),'fasta') if r.id in wanted)

SeqIO.write(records, output, "fasta")
